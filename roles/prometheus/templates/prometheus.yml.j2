global:
  scrape_interval: 15s 
  evaluation_interval: 15s
  scrape_timeout: 10s

scrape_configs:
  - job_name: prometheus
    static_configs:
      - targets: ["localhost:9090"]
  - job_name: node_exporter
    static_configs:
      - targets: ["localhost:9100"]
  - job_name: podman
    static_configs:
      - targets: ["127.0.0.1:9882"]
